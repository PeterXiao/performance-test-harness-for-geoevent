<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane 
	prefHeight="140" prefWidth="340" 
	stylesheets="@default.css"
	xmlns:fx="http://javafx.com/fxml" 
	fx:controller="com.esri.geoevent.test.performance.ui.ConfirmationDialogController">
  <children>
  	<GridPane AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" vgap="4" hgap="4">
    	<rowConstraints>
    		<RowConstraints prefHeight="40" valignment="CENTER"/>
    		<RowConstraints prefHeight="80" valignment="CENTER"/>
    	</rowConstraints>
      	<children>
        	<Label fx:id="confirmMsgLabel" styleClass="labels,required" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.halignment="CENTER" wrapText="true" textAlignment="CENTER"/>
	    	<HBox id="HBox" alignment="CENTER" minWidth="-1.0" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
		      <children>
		        <Button fx:id="okBtn" onAction="#handleOk" prefWidth="80.0" styleClass="buttons" />
		        <Button fx:id="cancelBtn" onAction="#handleCancel" prefWidth="80.0" styleClass="buttons" />
		      </children>
		    </HBox>
    	</children>
    </GridPane>
  </children>
</AnchorPane>
